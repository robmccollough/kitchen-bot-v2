{
  "name": "kitchen-bot-express",
  "version": "1.0.0",
  "description": "KitchenBot transitioned to follow react-app-structure with client and express server",
  "main": "./server/app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "nodemon ./server/app.js",
    "server": "nodemon ./server/app.js",
    "serverdev": "nodemon ./server/app.js --dev",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "jsonwebtoken": "^8.5.1",
    "localtunnel": "^2.0.0",
    "mongoose": "^5.7.3",
    "nodemon": "^1.19.3"
  }
}
